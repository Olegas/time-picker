  "devDependencies": {
    "@babel/cli": "^7.17.6",
    "@babel/core": "7.17",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.1",
    "@types/react": "^17",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-config-react-app": "^7.0.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.8",
    "parcel": "^2.4.1",
    "prettier": "^2.6.2",
    "react": "^17",
    "react-dom": "^17",
    "typescript": "^4.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.ts": [
      "prettier --write src/**/*",
      "eslint --fix src/**/*",
      "jest --bail --findRelatedTests"
    ]
  }
}
