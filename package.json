  "main": "index.js",
  "author": "Oleg Elifantiev <oleg@elifaniev.ru>",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "eslint": "eslint src/**/*",
    "eslint:fix": "eslint --fix src/**/*",
    "prettier": "prettier src/**/*",
